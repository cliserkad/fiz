type IfArgGiven {
  const NO_ARG : "No argument given"

  main {
    // if args has 0 or less arguments
    if args.size <= 0 {
      println(NO_ARG)
    } else {
      // if the first argument is not empty
      string s : args[0]
      if s {
        println(s)
       } else {
        println(NO_ARG)
       }
    }
  }
}
type IntCompare {
  const ASSERTION_PASS : "This should be printed 8 times"

  main {
    if 1 = 1 { assert true }

    if 1 != 0 { assert true }

    if 2 > 1 { assert true }

    if 1 < 2 { assert true }

    if 1 <= 1 { assert true }

    if 1 <= 2 { assert true }

    if 2 >= 2 { assert true }

    if 2 >= 1 { assert true }
  }
}
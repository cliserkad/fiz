type IfStringNotEmpty {
  const GOOD: "hello from IfStringNotEmpty"
  const FAIL: "This message was shown in error"

  main {
    // set string to ""
    string s
    // if s is not empty
    if s {
      println(FAIL)
    }

    string s2: "testing"
    // if s is not empty
    // should yield true as s is "testing"
    if s2 {
      println(GOOD)
    }
  }
}
type DoubleComparisons {
    main {
        double a: 0.0
        double b: 1.0

        if a = b {
            println("fail   $a == $b")
        } else {
            println("pass   $a == $b")
        }

        if a != b {
            println("pass   $a != $b")
        } else {
            println("fail   $a != $b")
        }

        if a < b {
            println("pass   $a < $b")
        } else {
            println("fail   $a < $b")
        }

        if a <= b {
            println("pass   $a <= $b")
        } else {
            println("fail   $a <= $b")
        }

        if a > b {
            println("fail   $a > $b")
        } else {
            println("pass   $a > $b")
        }

        if a >= b {
            println("fail   $a >= $b")
        } else {
            println("pass   $a >= $b")
        }

        println("done")

    }
}
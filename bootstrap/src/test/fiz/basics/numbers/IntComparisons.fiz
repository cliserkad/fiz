type IntComparisons {
    main {
        i32 a= 0
        i32 b= 1

        if a == b {
            println("fail   $a == $b")
        } else {
            println("pass   $a == $b")
        }

        if a != b {
            println("pass   $a != $b")
        } else {
            println("fail   $a != $b")
        }

        if a < b {
            println("pass   $a < $b")
        } else {
            println("fail   $a < $b")
        }

        if a <= b {
            println("pass   $a <= $b")
        } else {
            println("fail   $a <= $b")
        }

        if a > b {
            println("fail   $a > $b")
        } else {
            println("pass   $a > $b")
        }

        if a >= b {
